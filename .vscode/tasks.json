{
  "version": "0.1.0",
  "command": "go",
  "isShellCommand": true,
  "showOutput": "always",  
  "tasks": [
    {
      "taskName": "install",
      "suppressTaskName": false,
      "isBuildCommand": true,
      "args": [          
        "UlboraCmsV3/services",
        "github.com/gorilla/mux",
        "github.com/gorilla/sessions",
        "github.com/Ulbora/go-better-sessions",
        "github.com/Ulbora/go-oauth2-client",
        "github.com/dgrijalva/jwt-go"
      ]
    },
    {
      "taskName": "get",
      "suppressTaskName": false,
      "isBuildCommand": false,
      "args": [
        "-u",
        "github.com/gorilla/mux",
        "github.com/gorilla/sessions",
        "github.com/Ulbora/go-better-sessions",
        "github.com/Ulbora/go-oauth2-client",
        "github.com/dgrijalva/jwt-go"
      ]
    }
  ]
}